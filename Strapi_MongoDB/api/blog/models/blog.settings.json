{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "Imagen": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Descripcion": {
      "type": "richtext",
      "required": true
    },
    "categoria": {
      "model": "categorias"
    },
    "agente": {
      "model": "agentes"
    }
  }
}
